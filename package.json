{
  "name": "next-wallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "DEBUG=1 NEXTAUTH_DEBUG=1 node --inspect src/server.js | pino-pretty",
    "build": "next build",
    "clean": "rm -rf .next",
    "start": "NODE_ENV=production node src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check": "npm run vet && npm run lint",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "vet": "npmvet -r ci",
    "msg": "echo '! Remember to run \u001b[0;33mnpm run lint:fix\u001b[0m first!'",
    "lang:extract:base": "formatjs extract 'src/**/*.{ts,tsx}' --out-file lang/en.json",
    "lang:update:sv": "mv -f lang/sv.json lang/temp.json && jq -s '.[0] * .[1]' lang/en.json lang/temp.json > lang/sv.json && rm lang/temp.json",
    "lang:compile:en": "formatjs compile lang/en.json --ast --out-file src/locales/en.json",
    "lang:compile:sv": "formatjs compile lang/sv.json --ast --out-file src/locales/sv.json",
    "lang:extract": "npm run msg --silent && npm run lang:extract:base && npm run lang:update:sv",
    "lang:compile": "npm run lang:compile:en & npm run lang:compile:sv"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@formatjs/cli": "^5.1.3",
    "@formatjs/swc-plugin": "^1.4.14",
    "@inrupt/solid-client": "^1.23.3",
    "@mui/material": "^5.10.10",
    "@reduxjs/toolkit": "^1.8.6",
    "clsx": "^1.2.1",
    "jose": "^4.10.1",
    "next": "^12.3.1",
    "next-auth": "^4.14.0",
    "node-jq": "^2.3.4",
    "npmvet": "^0.2.0",
    "pino": "^8.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.1",
    "react-redux": "^8.0.4",
    "sharp": "^0.31.1",
    "swr": "^1.3.0",
    "uuid": "^9.0.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "commander": "^9.4.1",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-formatjs": "^4.3.4",
    "eslint-plugin-prettier": "^4.2.1",
    "history": "^5.3.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "msw": "^0.47.4",
    "pino-pretty": "^9.1.1",
    "typescript": "^4.8.4",
    "whatwg-fetch": "^3.6.2"
  }
}
