{
  "name": "next-wallet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_OPTIONS=--inspect NEXTAUTH_DEBUG=1 node src/server.js",
    "build": "next build",
    "start": "NODE_ENV=production node src/server.js",
    "lint": "eslint src",
    "lang:extract:base": "formatjs extract 'src/**/*.{ts,tsx}' --out-file lang/en.json",
    "lang:update:sv": "mv -f lang/sv.json lang/temp.json && jq -s '.[0] * .[1]' lang/en.json lang/temp.json > lang/sv.json && rm lang/temp.json",
    "lang:compile:en": "formatjs compile lang/en.json --ast --out-file src/locales/en.json",
    "lang:compile:sv": "formatjs compile lang/sv.json --ast --out-file src/locales/sv.json",
    "lang:extract": "npm run lang:extract:base && npm run lang:update:sv",
    "lang:compile": "npm run lang:compile:en & npm run lang:compile:sv"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@formatjs/cli": "^5.1.0",
    "@formatjs/swc-plugin": "^1.4.11",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-regular-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@inrupt/solid-client": "^1.23.2",
    "@inrupt/solid-client-authn-browser": "^1.12.2",
    "@mui/material": "^5.10.4",
    "@reduxjs/toolkit": "^1.8.5",
    "eslint-plugin-formatjs": "^4.3.1",
    "jose": "^4.9.1",
    "next": "12.2.5",
    "next-auth": "^4.10.3",
    "next-redux-wrapper": "^7.0.5",
    "node-jq": "^2.3.4",
    "random-js": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.0.8",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.0",
    "swr": "^1.3.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/node": "18.7.11",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "typescript": "4.7.4"
  }
}
